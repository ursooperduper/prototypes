tmux has-session -t proto

if [ $? != 0 ]
then
  tmux new-session -s proto -n editor -d
  tmux send-keys -t proto:1.1 'cd ~/Dropbox/code/prototypes' C-m
  tmux send-keys -t proto:1.1 'jekyll serve --watch --incremental' C-m

  tmux split-window -h -t proto:1.1
  tmux send-keys -t proto:1.2 'cd ~/Dropbox/code/prototypes' C-m
  tmux send-keys -t proto:1.2 'atom .' C-m
  tmux send-keys -t proto:1.2 'cl' C-m
  tmux send-keys -t proto:1.2 'ls' C-m

  tmux split-window -v -t proto:1.1 -d
  tmux send-keys -t proto:1.3 'cd ~/Dropbox/code/prototypes' C-m
  tmux send-keys -t proto:1.3 'gulp' C-m
fi

tmux attach -t proto
